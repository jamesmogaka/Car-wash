Manager:
    Splashscreen:
    Signin:
    MainMenu:
    Services:
    Staff:
    Members:
    AddCar:
    Records:
    Financial:
    ServiceRecords:
    UserRecords:
    CarRecords:
    About:

<FlatButton@ButtonBehavior+Label>:
    font_size: 14

<CButton@ButtonBehavior+Label>:
    id:cbutton
    size_hint_x:.05
    canvas.before:
        Ellipse:
            pos: self.pos
            size: root.size
            source: "user.png"

<SplashScreen>:
    name : 'splash'
    orientation: 'vertical'
    canvas.before:
        Rectangle:
            pos: self.pos
            size: root.size
            source: "4.jpg"
    Label:
        text:"CAR WASH MANAGEMENT SYSTEM"
        font_size:40
        font_name:'Alger.ttf'
        color:(0,0,0,1)

<Signin>:
    name : 'signin_win'
    BoxLayout:
        id: signin_win
        orientation: "vertical"
        spacing: 10
        space_x: self.size[0]/3
        canvas.before:
            Color:
                rgba: (1,1,1, 1)
            Rectangle:
                size: self.size
                pos: self.pos

        BoxLayout:
            size_hint_y: None
            height: 50
            canvas.before:
                Color:
                    rgba: (.2, .2, .2, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: "Log In !"
                bold: True
                size_hint_x: .9
            FlatButton:
                text: "x"
                size_hint_x: .1

        BoxLayout:
            orientation: 'vertical'
            padding: signin_win.space_x, 10
            #spacing: 20
            BoxLayout:
                orientation: "vertical"
                spacing: 10
                size_hint_y: None
                height: 100
                Label:
                    id: info
                    text: ''
                    markup: True
                    size_hint_y: None
                    height: 20
                TextInput:
                    id: username_field
                    hint_text: "Username"
                    multiline: False
                    focus: True
                    on_text_validate: pwd_field.focus = True
                TextInput:
                    id: pwd_field
                    hint_text: "Password"
                    multiline: False
                    password: True
                    on_text_validate: root.validate_user()
            Label:
                id: sp
                size_hint_y: None
                height: 40
            Button:
                text: "Sign In"
                size_hint_y: None
                height: 40
                background_color: (.2,.2,.2, 1)
                background_normal: ''
                on_release: root.validate_user()
            Label:
                id: sp2

<MainMenu>:
    name:'menu'
    canvas.before:
        Rectangle:
            pos: self.pos
            size: root.size
            source: "11.jpg"
    BoxLayout:
        id : mainmenu
        orientation: 'vertical'
        BoxLayout:
            id : userdetails
            size_hint_y:None
            height:60
            pos_hint:{"top":1}
            canvas.before:
                Color:
                    rgba: (.2,.2,.2, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos

            Label:
                text: ''
                size_hint_x:.4
            Label:
                text:''
                size_hint_x:.4

            CButton
                on_release:root.log_out()


        BoxLayout:
            orientation:'vertical'
            spacing: 10
            padding:[40,50]

            Button:
                text:'CarWash Members'
                font_size:30
                font_name:'Timesbd.ttf'
                background_color:(0,0,0,0)
                canvas.before:
                    Color:
                        rgba: .5, .5, .5, 1
                    Line:
                        width: 3
                        rectangle: self.x, self.y, self.width, self.height
                on_release: app.root.current = 'members'

            Button:
                text:'Carwash Services'
                font_size:30
                font_name:'Timesbd.ttf'
                background_color:(0,0,0,0)
                canvas.before:
                    Color:
                        rgba: .5, .5, .5, 1
                    Line:
                        width: 3
                        rectangle: self.x, self.y, self.width, self.height
                on_release: app.root.current = 'services'


            Button:
                text:'Staff'
                font_size:30
                font_name:'Timesbd.ttf'
                background_color:(0,0,0,0)
                canvas.before:
                    Color:
                        rgba: .5, .5, .5, 1
                    Line:
                        width: 3
                        rectangle: self.x, self.y, self.width, self.height
                on_release: app.root.current = 'staff'

            Button:
                text:'CarWash Records'
                font_size:30
                font_name:'Timesbd.ttf'
                background_color:(0,0,0,0)
                canvas.before:
                    Color:
                        rgba: .5, .5, .5, 1
                    Line:
                        width: 3
                        rectangle: self.x, self.y, self.width, self.height
                on_release: app.root.current = 'records'

            BoxLayout:
                orientation:'horizontal'
                spacing:10
                Button:
                    text:'About'
                    font_size:30
                    font_name:'Timesbd.ttf'
                    background_color:(0,0,0,0)
                    canvas.before:
                        Color:
                            rgba: .5, .5, .5, 1
                        Line:
                            width: 3
                            rectangle: self.x, self.y, self.width, self.height
                    on_release: app.root.current = 'about'


                Button:
                    text:'Quit Application'
                    font_size:30
                    font_name:'Timesbd.ttf'
                    background_color:(0,0,0,0)
                    canvas.before:
                        Color:
                            rgba: .5, .5, .5, 1
                        Line:
                            width: 3
                            rectangle: self.x, self.y, self.width, self.height
                    on_release: app.get_running_app().stop()
<Services>:
    name : 'services'
    canvas.before:
