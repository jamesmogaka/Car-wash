            pos: self.pos
            size: root.size
            source: "6.jpg"
    BoxLayout:
        orientation:'vertical'
        spacing:10
        padding:[40,50]


<AddCar>:
    name : 'car'
    canvas.before:
        Rectangle:
            pos: self.pos
            size: root.size
            source: "9.jpg"

    BoxLayout:
        id: car_details
        orientation:'vertical'
        padding:[40,50]
        spacing:10

        BoxLayout:
            id: header_cars
            size_hint_y: None
            height: 40
            canvas.before:
                Color:
                    rgba: (.2, .2, .2, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: 'Add Car Form'
                size_hint_x: .9
                bold: True
                color: (1,1,1,1)

        GridLayout:
            cols:2
            spacing:25
            Label:
                text: 'Registration No.'
            TextInput:
                id:reg_no
                multiline: False
            Label:
                text: 'Car Model'
            TextInput:
                id:model
                multiline: False
            Label:
                text : 'Car Color'
            TextInput:
                id:car_color
                multiline: False
            Label:
                text: "Owner's ID"
            TextInput:
                id:owner_id
                multiline: False

        BoxLayout:
            id: car_form_btns
            orientation: 'horizontal'
            size_hint_y:None
            height: 50
            spacing:10

            Button:
                text:'Add Car'
                on_release:root.add_car()


            Button:
                text:'Back'
                on_release:app.root.current = 'menu'


<Members>:
    name: 'members'
    BoxLayout:
        id:members_screen
        orientation: 'vertical'
        canvas.before:
            Color:
                rgba: (1,1,1, 1)
            Rectangle:
                size: self.size
                pos: self.pos

        BoxLayout:
            id: header
            size_hint_y: None
            height: 40
            canvas.before:
                Color:
                    rgba: (0, 0, 0, 1)
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text: 'Car Wash Management System'
                size_hint_x: .9
                bold: True
                color: (1,1,1,1)
        BoxLayout:
            padding: 10
            BoxLayout:
                id: member_details
                orientation: "vertical"
                size_hint_x: .8
                spacing: 10

                BoxLayout:
                    id: table_labels
                    size_hint_y: None
                    height: 40
                    canvas.before:
                        Color:
                            rgba: (.2,.2,.2, 1)
                        Rectangle:
                            size: self.size
                            pos: self.pos

                    FlatButton:
                        text: 'ID'
                        size_hint_x: .3
                    FlatButton:
                        text: 'First Name'
                        size_hint_x: .4
                    FlatButton:
                        text: 'Last Name'
                        size_hint_x: .3

                BoxLayout:
                    id: table_inputs
                    size_hint_y: None
                    height: 30
                    spacing: 5
                    TextInput:
                        id: id_inp
                        size_hint_x: .3
                        multiline:False
                    TextInput:
                        id: fname_inp
                        size_hint_x: .4
                        multiline: False
                    TextInput:
                        id: lname_inp
                        size_hint_x: .3
                        multiline:False
                BoxLayout:
                    id: member_btns
                    orientation:'horizontal'
                    size_hint_y:None
                    height:40
                    Button:
                        id:show_records
                        text: 'Show Members'
                        size_hint_x: .2
                        on_release: root.show_members()
                    Button:
                        id: addmember_btn
                        text:'Add Member'
                        on_release: root.add_member()


                BoxLayout:
                    id: members_list
                    orientation: 'vertical'
                    BoxLayout:
                        size_hint_y: None
                        height: 30
                        canvas.before:
                            Color:
                                rgba: (.2, .2,.2, 1)
                            Rectangle:
                                size: self.size
                                pos: self.pos
                        Label:
                            text: 'ID'
                            size_hint_x: .3
                        Label:
                            text: 'First Name'
                            size_hint_x: .4
                        Label:
                            text: 'Last Name'
                            size_hint_x: .3
                    GridLayout:
                        id: member_records
                        cols: 1

                BoxLayout:
                    id: final_buttons
                    size_hint_y: None
                    height: 30
                    spacing: 5
                    Button:
                        id: add_car
                        text:'Add Vehicle'
                        size_hint_y: None
                        height: 30
                        on_release: app.root.current = 'car'
                    Button:
                        id: member_back
                        text:'Back'
                        size_hint_y: None
                        height: 30
                        on_release: app.root.current = 'menu'
<MemberPopup>:
    Label:
