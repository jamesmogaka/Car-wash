        height:40
        pos_hint:{"x":.1, "y":.1}
        on_release:app.root.current = 'signin_win'
    Button:
        id:btn_no
        text:'No'
        size_hint_y:None
        size_hint_x:None
        width:100
        height:40
        pos_hint:{"x":.6, "y":.1}
        on_release:app.root.curret = 'menu'

